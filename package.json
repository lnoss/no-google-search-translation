{
  "name": "no-google-search-translation",
  "displayName": "No Google Search Translation",
  "version": "0.0.3",
  "description": "Disable Google Search results auto translation feature. Auto redirect away from Google Translate proxy to the original URL page.",
  "scripts": {
    "dev:firefox": "plasmo dev --target=firefox-mv3",
    "dev:chromium": "plasmo dev --target=chrome-mv3",
    "build": "pnpm run build:firefox && pnpm run build:chromium",
    "build:firefox": "plasmo build --target=firefox-mv3",
    "build:chromium": "plasmo build --target=chrome-mv3",
    "build:opera": "plasmo build --target=opera-mv3",
    "build:edge": "plasmo build --target=edge-mv3",
    "build:zip": "pnpm run build:firefox --zip && pnpm run build:chromium --zip"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.1",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "license": "GPL-3.0-only",
  "engines": {
    "node": "^20"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": "Firefox >= 109, Chrome >= 88, last 3 major versions, not dead"
      }
    }
  },
  "homepage": "https://github.com/lnoss/no-google-search-translation",
  "bugs": {
    "url": "https://github.com/lnoss/no-google-search-translation/issues"
  },
